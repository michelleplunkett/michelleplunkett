---
layout: feed
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

	<title>Michelle Plunkett's Blog</title>
	<link href="http://michelle-plunkett.com/blog.html/atom.xml" rel="self"/>
	<link href="http://michelle-plunkett.com/blog.html"/>
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>http://michelle-plunkett.com/blog.html</id>
	<author>
		<name>Michelle Plunkett</name>
		<email>michellejplunkett@gmail.com</email>
	</author>

	{% for post in site.posts %}
		<entry>
			<title>{{ post.title }}</title>
			<link href="http://michelle-plunkett.com{{ post.url }}"/>
			<updated>{{ post.date | date_to_xmlschema }}</updated>
			<id>http://michelle-plunkett.com/{{ post.id }}</id>
			<content type="html">{{ post.content | xml_escape }}</content>
		</entry>
	{% endfor %}

</feed>
